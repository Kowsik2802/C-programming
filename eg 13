#include <stdio.h>

int main() {
    int n, temp, r, rev = 0;

    printf("Enter the number: ");
    scanf("%d", &n);
    temp = n;  

    while (temp > 0) {
        r = temp % 10;
        rev = rev * 10 + r;
        temp = temp / 10;
    }

    if (n == rev) {
        printf("%d is a palindrome\n", n);
    } else {
        printf("%d is not a palindrome\n", n);
    }

    return 0;
}
