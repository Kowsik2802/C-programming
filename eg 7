  #include<stdio.h>
   int main(){
       int number;
       int result;
       printf("Enter the number");
       scanf("%d",&number);
       int i=1;
       while(i<=10){
           result=number*i;
           i++;
           printf("%d*%d=%d\n",number,i,result);
           
       }
   }
       
    #include<stdio.h>
   int main(){
       int number;
       int result;
       printf("Enter the number");
       scanf("%d",&number);
       int i=1;
       do{
           result=number*i;
           i++;
           printf("%d*%d=%d\n",number,i,result);
           
       }while(i<=10);
   }
